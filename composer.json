{
  "name": "azdanov/first",
  "type": "wordpress-theme",
  "description": "WordPress theme developed with Sage",
  "keywords": [
    "wordpress"
  ],
  "homepage": "https://github.com/azdanov/wordpress-first",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Zdanov",
      "email": "azdanov@pm.me",
      "homepage": "https://github.com/azdanov"
    }
  ],
  "require": {
    "php": ">=7.3",
    "composer/installers": "~1.0",
    "illuminate/support": "5.6.*",
    "roots/sage-lib": "~9.0.7",
    "soberwp/controller": "~2.1.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "roots/sage-installer": "~1.6",
    "squizlabs/php_codesniffer": "^2.8.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "Roots\\Sage\\Installer\\ComposerScript::postCreateProject"
    ],
    "test": [
      "./vendor/bin/phpcs"
    ]
  },
  "support": {
    "issues": "https://github.com/azdanov/wordpress-first/issues"
  }
}
